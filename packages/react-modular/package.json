{
  "name": "@pixi/react-modular",
  "version": "7.0.2",
  "description": "Package allowing custom configurations of pixi-react. Different versions of PixiJS can be combined with different versions of React",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pixijs/pixi-react.git",
    "directory": "packages/react"
  },
  "license": "MIT",
  "sideEffects": false,
  "main": "index.js",
  "module": "module.js",
  "jsnext:main": "module.js",
  "typings": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./module.js"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./index.js"
      }
    }
  },
  "files": [
    "dist/",
    "LICENSE",
    "README.md",
    "index.d.ts",
    "index.js",
    "module.js"
  ],
  "scripts": {
    "clean": "rimraf dist index.d.ts",
    "prebuild": "npm run clean",
    "build": "npm run build:types && npm run build:dev && npm run build:prod",
    "build:types": "tsc -p tsconfig.types.json",
    "build:dev": "cross-env NODE_ENV=development rollup -c",
    "build:prod": "cross-env NODE_ENV=production rollup -c",
    "test": "jest --silent",
    "test:ci": "jest --silent --runInBand",
    "test:types": "tsc --noEmit",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@pixi/react-invariant": "*"
  },
  "devDependencies": {
    "@pixi/react-types": "*"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "6d5b98bc6d7eafd1b0016e60a725b2b6c9f46a33"
}
