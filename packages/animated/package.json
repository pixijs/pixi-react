    "build:dev": "cross-env NODE_ENV=development rollup -c",
    "build:prod": "cross-env NODE_ENV=production rollup -c",
    "test": "jest --silent",
    "test:ci": "jest --silent --runInBand",
    "test:types": "tsc --noEmit",
    "test:watch": "jest --watch"
  },
  "sideEffects": false,
  "dependencies": {
    "@pixi/react": "^7.1.2"
  },
  "peerDependencies": {
    "pixi.js": ">=8.0.0",
    "@pixi/react": ">=8.0.0",
    "@react-spring/animated": ">=9.0.0",
    "prop-types": "^15.8.1",
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0",
    "react-spring": ">=9.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "6d5b98bc6d7eafd1b0016e60a725b2b6c9f46a33"
