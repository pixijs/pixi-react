{
  "name": "@pixi/react-animated",
  "version": "7.1.2",
  "description": "Animate your @pixi/react components with react-spring",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pixijs/pixi-react.git",
    "directory": "packages/animated"
  },
  "license": "MIT",
  "main": "index.js",
  "module": "module.js",
  "jsnext:main": "module.js",
  "typings": "./index.d.ts",
  "files": [
    "dist/",
    "LICENSE",
    "README.md",
    "index.d.ts",
    "index.js",
    "module.js"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "prebuild": "npm run clean",
