    "lint:fix": "eslint . --fix",
    "test": "jest --silent",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@babel/runtime": "^7.14.8",
    "pixi.js-legacy": "^6.0.0",
    "@pixi/react-pixi": "*"
  },
  "devDependencies": {
    "@types/react": "17.0.15",
    "@types/react-dom": "17.0.9",
    "@types/react-reconciler": "0.26.3",
    "eslint-config-pixi-react": "*",
    "tsconfig": "*"
  },
  "peerDependencies": {
    "@babel/runtime": "^7.14.8",
    "pixi.js-legacy": "^6.0.0",
    "prop-types": "^15.8.1",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
